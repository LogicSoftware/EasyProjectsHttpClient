        <ReferenceField Name="TaskID" ReferencedEntity="Task" ReferenceType="Optional"/>
        <ReferenceField Name="ProjectID" ReferencedEntity="Project" ReferenceType="Optional" AdditionalJoinConditions=" OR [Project].[ProjectID] = [Task].[ProjectID]"/>
        <ReferenceField Name="UserID" ReferencedEntity="User" ReferenceType="Mandatory"/>
      </ReferenceFields>
      <AdditionalPermissionsJoin>
        [TT_TaskPermissions] AS [AdditionalPermissions] ON ([{0}].[TaskId] = [AdditionalPermissions].[TaskID] AND [AdditionalPermissions].[UserID] = @{1})
        LEFT OUTER JOIN [VV_ProjectPermissions] AS [AdditionalProjectPermissions] ON ([{0}].[ProjectId] = [AdditionalProjectPermissions].[ProjectId] AND [AdditionalProjectPermissions].[UserID] = @{1})
      </AdditionalPermissionsJoin>
      <AdditionalPermissionsFields>
        <PermissionField Name="ISNULL([AdditionalPermissions].[CanEdit],ISNULL([AdditionalProjectPermissions].[CanEdit],0))" Alias="[CanEditEntity]" />        
      </AdditionalPermissionsFields>
      <AdditionalWhere>
        and (([Message].[TaskId] is not null and [AdditionalPermissions].[TaskID] is not null) or ([Message].ProjectId is not null and [AdditionalProjectPermissions].ProjectID is not null))
      </AdditionalWhere>
    </EntityClass>
                                                                                                             
