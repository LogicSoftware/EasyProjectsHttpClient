        <PermissionField Name="[AdditionalPermissions].[CanManageAssigneesRequests]" Alias="[CanManageAssigneesRequests]" />
        <PermissionField Name="[AdditionalPermissions].[CanEditAllTasks]" Alias="[CanEditAllTasks]" />
        <PermissionField Name="[AdditionalPermissions].[CanEditAllIssues]" Alias="[CanEditAllIssues]" />
        <PermissionField Name="[AdditionalPermissions].[CanEditAllRequests]" Alias="[CanEditAllRequests]" />
        <PermissionField Name="[AdditionalPermissions].[CanEditAssignedTasks]" Alias="[CanEditAssignedTasks]" />
        <PermissionField Name="[AdditionalPermissions].[CanEditAssignedIssues]" Alias="[CanEditAssignedIssues]" />
        <PermissionField Name="[AdditionalPermissions].[CanEditAssignedRequests]" Alias="[CanEditAssignedRequests]" />
        <PermissionField Name="[AdditionalPermissions].[IsPortfolioManager]" Alias="[IsPortfolioManager]" />
      </AdditionalPermissionsFields>
      <Fields>
        <Field Name="ProjectID" AccessType="ReadOnly" EnableAPI="true">
