    <EntityClass Name="Portfolio"
         HasVisibilityPermission="CanSee_Portfolio"
         TableName="TT_Portfolios" PrimaryKeyField="PortfolioID" PermissionTable="TT_PortfolioPermissions" EnableAPI="true" EnableCustomFields="true">
      <Fields>
        <Field Name="PortfolioID" AccessType="ReadOnly"  EnableAPI="true">
          <Descriptor DataType="Integer" AccessType="ReadOnly" />
        </Field>
        <Field Name="CreationDate" AccessType="Optional">
          <Descriptor DataType="Date" AccessType="ReadOnly" FormatString="{0:d}"/>
        </Field>
        <Field Name="Name" AccessType="Mandatory"  EnableAPI="true">
          <Descriptor DataType="String" AccessType="Mandatory" OpensEntity="true" MaxLength="100" />
        </Field>
        <Field Name="Description" AccessType="Optional"  EnableAPI="true">
          <Descriptor DataType="HtmlText" AccessType="Mandatory" MaxLength="400000"/>
        </Field>
        <Field Name="Managers" Expression="dbo.ep_GetPortfolioManagers({0}.PortfolioID, @CurrentUserID, ' , ', {0}.AccountId)" AccessType="ReadOnly"  EnableAPI="true">
          <Descriptor DataType="String" AccessType="ReadOnly"/>
        </Field>
        <Field Name="ManagersComma" Expression="dbo.ep_GetPortfolioManagers({0}.PortfolioID, @CurrentUserID, ' , ', {0}.AccountId)" AccessType="ReadOnly" >
          <Descriptor DataType="String" AccessType="ReadOnly"/>
        </Field>
        <Field Name="ManagersSeparateLines" Expression="dbo.ep_GetPortfolioManagers({0}.PortfolioID, @CurrentUserID, char(10), {0}.AccountId)" AccessType="ReadOnly">
          <Descriptor DataType="String" AccessType="ReadOnly"/>
        </Field>
        <Field Name="Members" Expression="dbo.ep_GetPortfolioMembers({0}.PortfolioID, @CurrentUserID, {0}.AccountId)" AccessType="ReadOnly"  EnableAPI="true">
          <Descriptor DataType="String" AccessType="ReadOnly"/>
        </Field>
        <Field Name="HasProjects" Expression="(CASE WHEN EXISTS (SELECT TT_Projects.* FROM TT_Projects 
                  INNER JOIN TT_ProjectStatuses as ps ON TT_Projects.ProjectStatusID = ps.ProjectStatusID and TT_Projects.AccountID = ps.AccountID
                  WHERE PortfolioID = {0}.PortfolioID and (ps.InternalProjectStatusID &lt;&gt; 2 or ps.InternalProjectStatusID IS NULL)
                  AND TT_Projects.AccountID = {0}.AccountID
                  ) THEN 1 ELSE 0 END)" AccessType="ReadOnly" EnableAPI="true">
          <Descriptor DataType="Integer" AccessType="ReadOnly"/>
        </Field>
      </Fields>
      <ReferenceFields>
        <ReferenceField Name="CreatorID" ReferencedEntity="CreatorUser" ReferenceType="Mandatory"/>
        <ReferenceField Name="PortfolioStatusID" ReferencedEntity="PortfolioStatus" ReferenceType="Mandatory" />
        <ReferenceField Name="PortfolioCategoryID" ReferencedEntity="PortfolioCategory" ReferenceType="Optional"/>
      </ReferenceFields>
      <AdditionalPermissionsJoin>[VV_PortfolioMemberPermissions] AS [AdditionalPermissions] ON ([{0}].[AccountId] = [AdditionalPermissions].[AccountId] AND [{0}].[PortfolioID] = [AdditionalPermissions].[PortfolioID] AND [AdditionalPermissions].[UserID] = @{1})</AdditionalPermissionsJoin>
      <AdditionalPermissionsFields>
        <PermissionField Name="[AdditionalPermissions].[CanView]" Alias="CanViewPortfolioObjects" />
        <PermissionField Name="[AdditionalPermissions].[CanEdit]" Alias="CanEditPortfolioObjects" />
        <PermissionField Name="[AdditionalPermissions].[IsPortfolioManager]" Alias="IsPortfolioManager" />
        <PermissionField Name="[AdditionalPermissions].[IsPortfolioMember]" Alias="IsPortfolioMember" />
      </AdditionalPermissionsFields>
    </EntityClass>

