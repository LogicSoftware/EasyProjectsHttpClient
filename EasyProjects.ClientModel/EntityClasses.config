      <EntityClass Name="Portfolio"
				   HasVisibilityPermission="CanSee_Portfolio"
				   TableName="TT_Portfolios" PrimaryKeyField="PortfolioID" PermissionTable="TT_PortfolioPermissions" EnableSharedServicesSupport="true" EnableAPI="true">
