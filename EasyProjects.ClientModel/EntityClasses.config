          <Descriptor DataType="String" AccessType="ReadOnly" />
        </Field>
        <Field Name="FileLength" AccessType="ReadOnly">
          <Descriptor DataType="Integer" AccessType="ReadOnly" />
        </Field>
        <Field Name="UserID" AccessType="Mandatory" >
          <Descriptor DataType="Integer" AccessType="Mandatory" />
        </Field>
        <Field Name="UserName" AccessType="Mandatory" Expression="(SELECT [Name] FROM [TT_Users] WHERE [TT_Users].[UserId] = {0}.[UserID])">
          <Descriptor DataType="String" AccessType="Mandatory" />
        </Field>
        <Field Name="Email" AccessType="Mandatory" Expression="(SELECT [Email] FROM [TT_Users] WHERE [TT_Users].[UserId] = {0}.[UserID])">
          <Descriptor DataType="String" AccessType="Mandatory" />
        </Field>
        <Field Name="PostDate" AccessType="Mandatory">
          <Descriptor DataType="Date" AccessType="Mandatory" />
        </Field>
        <Field Name="AvatarHash" AccessType="Mandatory" Expression="(SELECT [AvatarHash] FROM [TT_Users] WHERE [TT_Users].[UserId] = {0}.[UserID])">
          <Descriptor DataType="String" AccessType="Mandatory" />
        </Field>
      </Fields>
      
      <AdditionalPermissionsJoin>
        [TT_TaskPermissions] AS [AdditionalPermissions] ON ([{0}].[TaskID] = [AdditionalPermissions].[TaskID] AND [AdditionalPermissions].[UserID] = @{1})
        LEFT OUTER JOIN [VV_ProjectPermissions] AS [AdditionalProjectPermissions] ON ([{0}].[ProjectID] = [AdditionalProjectPermissions].[ProjectId] AND [AdditionalProjectPermissions].[UserID] = @{1})
      </AdditionalPermissionsJoin>
      <AdditionalPermissionsFields>
        <PermissionField Name="(CASE [ClientsPortalMessages].[UserID]  WHEN @CurrentUserID THEN  1 ELSE ISNULL([AdditionalPermissions].[CanEdit],ISNULL([AdditionalProjectPermissions].[CanEdit],0)) END)" Alias="[IsCanEdit]" />
        <PermissionField Name="(CASE [ClientsPortalMessages].[UserID]  WHEN @CurrentUserID THEN  1 ELSE ISNULL([AdditionalPermissions].[CanDelete],ISNULL([AdditionalProjectPermissions].[CanDelete],0)) END)" Alias="[IsCanDelete]" />
      </AdditionalPermissionsFields>
      <AdditionalWhere>
        and (([ClientsPortalMessages].[TaskID] is not null and [AdditionalPermissions].[TaskID] is not null) or ([ClientsPortalMessages].[ProjectID] is not null and [AdditionalProjectPermissions].ProjectID is not null))
      </AdditionalWhere>
      
    </EntityClass>

