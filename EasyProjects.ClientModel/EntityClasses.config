            <Descriptor DataType="String" AccessType="Mandatory" OpensEntity="true" MaxLength="100" />
          </Field>
          <Field Name="Description" AccessType="Optional"  EnableAPI="true">
            <Descriptor DataType="HtmlText" AccessType="Mandatory" MaxLength="400000"/>
          </Field>
          <Field Name="Managers" Expression="dbo.ep_GetPortfolioManagers({0}.PortfolioID, @CurrentUserID, ' ')" AccessType="ReadOnly"  EnableAPI="true">
            <Descriptor DataType="String" AccessType="ReadOnly"/>
          </Field>
          <Field Name="ManagersComma" Expression="dbo.ep_GetPortfolioManagers({0}.PortfolioID, @CurrentUserID, ' , ')" AccessType="ReadOnly" >
            <Descriptor DataType="String" AccessType="ReadOnly"/>
          </Field>
          <Field Name="ManagersSeparateLines" Expression="dbo.ep_GetPortfolioManagers({0}.PortfolioID, @CurrentUserID, char(10))" AccessType="ReadOnly">
            <Descriptor DataType="String" AccessType="ReadOnly"/>
          </Field>
          <Field Name="Members" Expression="dbo.ep_GetPortfolioMembers({0}.PortfolioID, @CurrentUserID)" AccessType="ReadOnly"  EnableAPI="true">
            <Descriptor DataType="String" AccessType="ReadOnly"/>
          </Field>
		  <Field Name="HasProjects" Expression="(CASE WHEN EXISTS (SELECT * FROM TT_Projects WHERE PortfolioID = {0}.PortfolioID) THEN 1 ELSE 0 END)" AccessType="ReadOnly" EnableAPI="true">
			<Descriptor DataType="Integer" AccessType="ReadOnly"/>
		  </Field>
		</Fields>
		<ReferenceFields>
		  <ReferenceField Name="CreatorID" ReferencedEntity="CreatorUser" ReferenceType="Mandatory"/>
		  <ReferenceField Name="PortfolioStatusID" ReferencedEntity="PortfolioStatus" ReferenceType="Mandatory" />
		  <ReferenceField Name="PortfolioCategoryID" ReferencedEntity="PortfolioCategory" ReferenceType="Optional"/>
    </ReferenceFields>
        <AdditionalPermissionsJoin>[VV_PortfolioMemberPermissions] AS [AdditionalPermissions] ON ([{0}].[PortfolioID] = [AdditionalPermissions].[PortfolioID] AND [AdditionalPermissions].[UserID] = @{1})</AdditionalPermissionsJoin>
        <AdditionalPermissionsFields>
          <PermissionField Name="[AdditionalPermissions].[CanView]" Alias="[CanViewPortfolioObjects]" />
          <PermissionField Name="[AdditionalPermissions].[CanEdit]" Alias="[CanEditPortfolioObjects]" />
          <PermissionField Name="[AdditionalPermissions].[IsPortfolioManager]" Alias="[IsPortfolioManager]" />
