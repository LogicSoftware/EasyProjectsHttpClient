    <EntityClass Name="Project" TableName="TT_Projects" PrimaryKeyField="ProjectID" PermissionTable="VV_ProjectPermissions" EnableSharedServicesSupport="true" EnableAPI="true">
      <AdditionalPermissionsJoin>
        (SELECT [CanManageMembers].CanManageProjectMembers as CanManageProjectMembersCalculated,
        [Permissions].*
        FROM ep_GetCanManageProjectMembersForAllProjects(@{1}) as [CanManageMembers]
        LEFT JOIN [VV_ProjectMemberPermissions] AS [Permissions] ON [Permissions].[ProjectID] = [CanManageMembers].[ProjectID] AND [Permissions].[UserID] = @{1}
        ) AS [AdditionalPermissions] ON ([{0}].[ProjectID] = [AdditionalPermissions].[ProjectID])
      </AdditionalPermissionsJoin>
      <AdditionalPermissionsFields>
        <PermissionField Name="[AdditionalPermissions].[CanManageProjectMembersCalculated]" Alias="[CanManageProjectMembersCalculated]" />
