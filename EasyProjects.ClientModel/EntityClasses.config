    <EntityClass Name="Message" TableName="TT_Messages" PrimaryKeyField="MessageID" EnableAPI="true">
      <Fields>
        <Field Name="MessageID" AccessType="ReadOnly" EnableAPI="true">
          <Descriptor DataType="Integer" AccessType="ReadOnly"/>
        </Field>
        <Field Name="PostDate" AccessType="Mandatory" EnableAPI="true">
          <Descriptor DataType="Date" AccessType="Mandatory"/>
        </Field>
        <Field Name="MessageText" AccessType="Mandatory" EnableAPI="true">
          <Descriptor DataType="MultilineText" AccessType="Mandatory" MaxLength="2000"/>
        </Field>
        <Field Name="EditedAt" AccessType="Optional">
          <Descriptor DataType="Date" AccessType="Optional"/>
        </Field>
        <Field Name="EditedByName" AccessType="Optional">
          <Descriptor DataType="String" AccessType="Optional" />
        </Field>
        <!-- This field just for log. We use EditedByName to display user name that edit message-->
        <Field Name="EditedByUserID" AccessType="Optional">
          <Descriptor DataType="Integer" AccessType="Optional"/>
        </Field>
      </Fields>
      <ReferenceFields>
        <ReferenceField Name="TaskID" ReferencedEntity="Task" ReferenceType="Optional"/>
        <ReferenceField Name="ProjectID" ReferencedEntity="Project" ReferenceType="Optional" AdditionalJoinConditions=" OR ([{1}].[ProjectID] = [{2}].[ProjectID] AND [{1}].[AccountId] = [{0}].[AccountId])" AdditionalJoinReference="TaskID"/>
        <ReferenceField Name="UserID" ReferencedEntity="User" ReferenceType="Optional"/>
      </ReferenceFields>
      <AdditionalPermissionsJoin>
        [TT_TaskPermissions] AS [AdditionalPermissions] ON ([{0}].[AccountId] = [AdditionalPermissions].[AccountId] AND [{0}].[TaskId] = [AdditionalPermissions].[TaskID] AND [AdditionalPermissions].[UserID] = @{1})
        LEFT OUTER JOIN [TT_ProjectPermissions] AS [AdditionalProjectPermissions] ON ([{0}].[AccountId] = [AdditionalProjectPermissions].[AccountId] AND [{0}].[ProjectId] = [AdditionalProjectPermissions].[ProjectId] AND [AdditionalProjectPermissions].[UserID] = @{1})
      </AdditionalPermissionsJoin>
      <AdditionalPermissionsFields>
        <PermissionField Name="ISNULL([AdditionalPermissions].[CanEdit],ISNULL([AdditionalProjectPermissions].[CanEdit],0))" Alias="CanEditEntity" />
      </AdditionalPermissionsFields>
      <AdditionalPermissionsWhere>
        and (([Message].[TaskId] is not null and [AdditionalPermissions].[TaskID] is not null) or ([Message].ProjectId is not null and [AdditionalProjectPermissions].ProjectID is not null))
      </AdditionalPermissionsWhere>
    </EntityClass>

